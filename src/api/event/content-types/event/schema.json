{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Date": {
      "type": "datetime"
    },
    "Price": {
      "type": "decimal"
    },
    "Location": {
      "type": "string"
    },
    "Picture": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket.ticket",
      "mappedBy": "event"
    }
  }
}
